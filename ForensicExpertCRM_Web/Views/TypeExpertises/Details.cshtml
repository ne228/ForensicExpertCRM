@model TypeExpertise

@{
    ViewData["Title"] = "Тип экспертизы";
}

<h1 align="center">Тип экспертизы</h1>

<div>
    <hr />
    <div class="details">
        <div class="details-item">
            <div class="details-item-label">
                Название
            </div>

            <div class="details-item-value">
                @Model.Name
            </div>
        </div>



        <div class="details-item">
            <div class="details-item-label">
                Эксперты
            </div>
            @foreach (var expert in Model.Experts)
            {
                <div class="">
                    @expert.Name
                </div>
            }
        </div>

    </div>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Изменить</a> |
    <a asp-action="Index">Вернуться к списку</a>
</div>